{
  "name": "@v2018/server",
  "version": "1.0.0",
  "description": "V2018 NodeJS server",
  "bin": {
    "wwtool": "dist/main.js"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "cleandist": "rm -r ./dist",
    "start": "node dist/main.js",
    "serve": "ts-node-dev src/main.ts",
    "dev": "ts-node-dev src/main.ts",
    "pkg": "pkg --targets node14-win-x64 --output tool.exe package.json",
    "tscversion": "tsc --version"
  },
  "author": "R.W. Bruining",
  "license": "MIT",
  "dependencies": {
    "@v2018/shared": "^1.0.0",
    "@nestjs/common": "^7.4.4",
    "@nestjs/core": "^7.4.4",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/serve-static": "^2.1.3",
    "axios": "^0.20.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.20",
    "reflect-metadata": "^0.1.13",
    "ws": "^7.3.1",
    "yn": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/fs-extra": "^9.0.1",
    "@types/node": "^14.11.2",
    "@types/ws": "^7.2.7",
    "pkg": "^4.4.9",
    "ts-node-dev": "1.0.0-pre.63",
    "typescript": "^4.0.3"
  },
  "pkg": {
    "assets": [
      "scripts/**/*",
      "images/**/*"
    ]
  }
